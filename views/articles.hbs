<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>{{title}}</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="/public/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="/public/styles/index.css" media="all">
    <link rel="stylesheet" href="/public/styles/common.css" media="all">
    <script src="/public/layui/layui.js"></script>
</head>

<body>
    <div class="layui-layout layui-layout-admin">
        {{> header}}
        {{> panel}}
        <div class="container layui-row">
            <div class="container-left layui-col-md8">
                <div class="container-inner">
                    <div class="panel-title">
                        <span class="layui-breadcrumb" lay-separator=" | ">
                            <a href="">综合</a>
                            <a href="">热门</a>
                            <a href="">评论</a>
                        </span>
                    </div>
                    <ul class="article-lists">
                        {{#each lists}}
                        <li class="article-item ">
                            <a href="/render/write/{{WriteID}}">
                                <img src="data:image/jpeg;base64,"
                                    alt="">
                                <p class="layer1">
                                    <a href="/render/write/{{WriteID}}" class="title">{{Title}}</a>
                                    <span class="time">{{CreateTime}}</span>
                                    <span class="tags">
                                        <span class="layui-badge layui-bg-black"><a href="">react</a></span>
                                        <span class="layui-badge layui-bg-black"><a href="">javascript</a></span>
                                    </span>
                                </p>
                                <p class="layer2">
                                    <a href="/render/write/{{WriteID}}">{{Description}}</a>
                                </p>
                            </a>
                        </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            <div class="container-right layui-col-md4">
                <div class="container-inner">
                    <div class="list-one">
                        <div class="title">
                            热门
                        </div>
                        {{#each hots}}
                        <a class="article-item" href="/render/write/{{WriteID}}">
                            <span>{{Title}}</span>
                        </a>
                        {{/each}}
                    </div>
                    <div class="list-two">
                        <div class="title">
                            标签
                        </div>
                        <div class="layui-row layui-col-space10 tags">
                            {{#each hots}}
                            <a class="article-item">
                                <button type="button" class="layui-btn layui-btn-xs">{{Title}}</button>
                            </a>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{> script}}
    <script>
        layui.use('element', function () {
            var element = layui.element;
        });

        init()

        function init() {
            logged()
        }
    </script>

</html>